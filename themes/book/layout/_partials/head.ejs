<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no">

    <% if (theme.favicon_url) { %>
      <link rel="icon" href="<%- url_for(theme.favicon_url) %>" />
    <% } %>

    <title>
        <% if (page.title) { %>
          <%= page.title + ' - ' + config.title %>
        <% } else{%>
          <%= config.title %>
        <% } %>
    </title>

    <meta name="description" content="<%= config.description %>">

    <!-- Spectre.css framework -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/spectre.css@0.5.8/dist/spectre.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/spectre.css@0.5.8/dist/spectre-exp.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/spectre.css@0.5.8/dist/spectre-icons.min.css">

    <!-- Noto Sans SC -->
    <link href="https://fonts.loli.net/css?family=Noto+Sans+SC" rel="stylesheet">
    <!-- Noto Sans -->
    <link href="https://fonts.loli.net/css?family=Noto+Sans" rel="stylesheet">

    <!-- theme css & js -->
    <%- css('css/book') %>
    <%- js('js/book') %>

    <!-- tocbot -->
    <script src="https://cdn.jsdelivr.net/npm/tocbot@4.10.0/dist/tocbot.min.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/tocbot@4.10.0/dist/tocbot.css">
    <meta property="og:type" content="website" />
    <meta property="og:title" content="<%= config.title %>">
    <meta property="og:description" content="<%= config.description %>">
    <meta property="og:image" content="https://remoteok.cn/thumbnail.png">
    <meta property="og:url" content="https://remoteok.cn/">

    <%- partial('_lib/google-analytics') %>
    <%- partial('_lib/zooming-image') %>
</head>